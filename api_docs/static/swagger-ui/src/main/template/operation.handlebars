  <ul class='operations' >
    <li class='{{method}} operation' id='{{parentId}}_{{nickname}}'>
      <div class='heading'>
        <div class="topHeading">
          <div class='httpMethod {{method}}'>
            <a href='#!/{{encodedParentId}}/{{nickname}}' class="toggleOperation">{{method}}</a>
          </div>
          <div class="operationId">
            <a href='#!/{{encodedParentId}}/{{nickname}}' class="toggleOperation">{{{nickname}}}</a>
          </div>
        </div>
      </div>
      <div class='content' id='{{parentId}}_{{nickname}}_content' style='display:none'>
        <div class="path">
          <h3>
            <a href='#!/{{encodedParentId}}/{{nickname}}' class="toggleOperation {{#if deprecated}}deprecated{{/if}}">
              {{path}}
            </a>
          </h3>
        </div>
        {{#if deprecated}}
            <h4><span data-sw-translate>Warning: Deprecated</span></h4>
        {{/if}}
        {{#if description}}
        <div class="markdown">{{{description}}}</div>
        {{/if}}
        {{#oauth}}
        <div class="auth">
        <span class="api-ic ic-error">{{/oauth}}
        {{#each oauth}}
          <div class="api_information_panel">
          {{#each this}}
            <div title='{{{this.description}}}'>{{this.scope}}</div>
          {{/each}}
          </div>
        {{/each}}
        {{#oauth}}</span></div>{{/oauth}}
        {{#oauth}}
        <div class='access'>
          <span class="api-ic ic-off" title="click to authenticate"></span>
        </div>
        {{/oauth}}
        {{#if headers}}
          <h4 data-sw-translate>Headers</h4>
          <table class="headers">
            <thead>
              <tr>
                <th style="width: 100px; max-width: 100px" data-sw-translate>Header</th>
                <th style="width: 310px; max-width: 310px" data-sw-translate>Description</th>
                <th style="width: 200px; max-width: 200px" data-sw-translate>Type</th>
                <th style="width: 320px; max-width: 320px" data-sw-translate>Other</th>
              </tr>
            </thead>
            <tbody>
              {{#each headers}}
              <tr>
                <td>{{@key}}</td>
                <td>{{this.description}}</td>
                <td>{{this.type}}</td>
                <td>{{this.other}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        {{/if}}

        <form accept-charset='UTF-8' class='sandbox'>
          <div style='margin:0;padding:0;display:inline'></div>
          {{#if parameters}}
          <h4 data-sw-translate>Parameters</h4>
          <table class='fullwidth'>
          <thead>
            <tr>
            <th style="width: 100px; max-width: 100px" data-sw-translate>Parameter</th>
            <th style="width: 210px; max-width: 210px" data-sw-translate>Value</th>
            <th style="width: 200px; max-width: 200px" data-sw-translate>Description</th>
            <th style="width: 150px; max-width: 150px" data-sw-translate>Parameter Type</th>
            <th style="width: 170px; max-width: 170px" data-sw-translate>Data Type</th>
            </tr>
          </thead>
          <tbody class="operation-params">

          </tbody>
          </table>
          {{/if}}
          {{#if type}}
            <div class="response-class">
              <h4><span data-sw-translate>Response Class</span> (<span data-sw-translate>Status</span> {{successCode}})</h4>
              <p><span class="model-signature" /></p>
              <div class="response-content-type" />
              </div>
          {{/if}}
          {{!-- {{#if responseMessages}}
          <div style='margin:0;padding:0;display:inline'></div>
          <h4 data-sw-translate>Error Response Messages</h4>
          <table class='fullwidth'>
            <thead>
            <tr>
              <th data-sw-translate>HTTP Status Code</th>
              <th data-sw-translate>Reason</th>
              <th data-sw-translate>Response Model</th>
            </tr>
            </thead>
            <tbody class="operation-status">
            </tbody>
          </table>
          {{/if}} --}}

          {{#if isReadOnly}}
          {{else}}
          <div class='sandbox_header'>
            <input class='submit swagger-btn' type='submit' value='Try it out!' data-sw-translate/>
            <a href='#' class='response_hider swagger-btn' style='display:none' data-sw-translate>Hide Response</a>
            <span class='response_throbber' style='display:none'></span>
          </div>
          {{/if}}
        </form>
        <div class='response' style='display:none'>
          <h4 class='curl'>Curl</h4>
          <div class='block curl'></div>
          <h4 data-sw-translate>Request URL</h4>
          <div class='block request_url'></div>
          {{#if showRequestHeaders}}
          <h4 data-sw-translate>Request Headers</h4>
          <div class='block request_headers'></div>
          {{/if}}
          <h4 data-sw-translate>Response Body</h4>
          <div class='block response_body'></div>
          <h4 data-sw-translate>Response Code</h4>
          <div class='block response_code'></div>
          <h4 data-sw-translate>Response Headers</h4>
          <div class='block response_headers'></div>
        </div>
      </div>
    </li>
  </ul>
